{
  "name": "tsnewprojecttemplate",
  "version": "0.1.0",
  "description": "Framework for new Typescript apps",
  "main": "./dist/index.js",
  "typings": "./dist/typings",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "NODE_ENV=local node ./dist/main -env local",
    "dist": "sh ./scripts/dist.sh",
    "test": "mocha test/**/*.test.ts",
    "testwithmocks": "./scripts/runtests.sh mock",
    "lint": "./node_modules/.bin/eslint src/*.ts ./*.ts",
    "patch": "npm version patch",
    "publish": "npm publish",
    "unpublish": "npm unpublish tsnewprojecttemplate --force",
    "knex-latest": "knex migrate:latest",
    "knex-rollback": "knex migrate:rollback",
    "knex-seed": "knex seed:run",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "author": "Marc Adler <marc.adler@ctoasaservice.org>",
  "license": "ISC",
  "keywords": [
    "TSNewProjectTemplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://magmasystems.github.com/TSNewProjectTemplate/_git/TSNewProjectTemplate"
  },
  "publishConfig": {},
  "pre-commit": [
    "precommit-msg",
    "testwithmocks"
  ],
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/knex": "^0.16.1",
    "@types/mock-knex": "^0.4.3",
    "@types/node": "^12.19.9",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.14.7",
    "@types/postman-collection": "^3.0.1",
    "@types/socket.io": "^1.4.31",
    "@types/ws": "^6.0.1",
    "@types/x-ray": "^2.3.1",
    "acorn": "^8.0.4",
    "aws-sdk": "^2.811.0",
    "body-parser": "^1.19.0",
    "bson": "^4.2.2",
    "eventemitter2": "^5.0.0",
    "express": "^4.17.1",
    "express-oas-generator": "^1.0.30",
    "knex": "^0.21.13",
    "log4js": "^3.0.6",
    "method-override": "^2.3.9",
    "mock-knex": "^0.4.9",
    "mongoose": "^5.11.8",
    "mongoose-to-swagger": "^1.1.1",
    "objection": "^2.2.3",
    "pg": "^8.5.1",
    "postman-collection": "^3.3.0",
    "proxy-agent": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "serve-favicon": "^2.4.4",
    "socket.io": "^2.0.4",
    "stylus": "^0.54.5",
    "typescript-logging": "^0.6.3",
    "uuid": "^3.1.0",
    "ws": "^6.1.3",
    "x-ray": "^2.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.48",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "aws-sdk-mock": "^5.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "pre-commit": "^1.2.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.9.7"
  }
